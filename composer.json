{
  "name": "sifo/sifo-playground",
  "description": "Playground APP using SIFO Framework",
  "license": "Apache-2.0",
  "keywords": [],
  "homepage": "https://github.com/sifophp/SIFO",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/uvinum/SIFO"
    }
  ],
  "require": {
    "php": ">=7.2",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "sifophp/sifo": "dev-psr7-request-response",
    "zendframework/zend-diactoros": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Sifo\\Playground\\": "instances/sifoplayground/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sifo\\Playground\\Tests\\": "instances/sifoplayground/tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require-dev": {
    "symfony/var-dumper": "^4.2"
  }
}
